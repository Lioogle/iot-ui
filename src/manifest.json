{
  "package": "com.application.iot-ui",
  "name": "iot-ui",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1010",
  "icon": "/Common/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "home",
    "pages": {
      "home": {
        "component": "index"
      },
      "testButtons": {
        "component": "index"
      },
      "testNavBar": {
        "component": "index"
      },
      "About": {
        "component": "index"
      },
      "testSwitch": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      },
      "DemoDetail": {
        "titleBarText": "详情页"
      },
      "About": {
        "menu": false
      }
    }
  }
}